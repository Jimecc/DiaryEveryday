1: null,\n      value2: null,\n      value3: new Date(),\n      show_date: false,\n      show_weather: false,\n      drawer: false,\n      article: '',\n      colors: ['#99A9BF', '#F7BA2A', '#FF9900']  // 等同于 { 2: '#99A9BF', 4: { value: '#F7BA2A', excluded: true }, 5: '#FF9900' }\n    }\n  },\n  methods:{\n    click_show_date(){\n      if(this.show_date ==false){\n        this.show_date = true;\n\n      }else{\n        this.show_date = false;\n      }\n\n    },\n    click_show_weather(){\n      this.show_weather = !this.show_weather;\n    },\n    goBack(){\n      this.$router.replace('/');\n    },\n    click_collect(){\n      if(this.articleForm.collection == 1){\n        this.articleForm.collection = 0;\n        this.icons.collection = 'el-icon-star-off';\n        this.istyle.collection = 'margin-left: 10px;width:50px;height:50px;font-size:18px';\n      }else{\n        this.articleForm.collection = 1;\n        this.icons.collection = 'el-icon-star-on';\n        this.istyle.collection = 'margin-left: 10px;width:50px;height:50px;background-color:#ecf5ff;color:#ff7f50;font-size:18px';\n      }\n    }\n\n  }\n}\n</script>\n\n<style scoped>\n.div_1{\n  width: 100%;\n  height: 100%;\n  position: absolute;\n  /*background-color: blanchedalmond;*/\n}\n.header_1{\n  margin-top: 20px;\n}\n.input_title{\n  width: 100%;\n  border: lightslategray 1px solid;\n  border-radius: 5px;\n}\n.aside_1{\n  width: 250px;\n  padding: 30px;\n  /*background-color: coral;*/\n}\n.el-divider--vertical{\n  display: inline-block;\n  width: 1px;\n  height: 800px;\n  margin: 0 8px;\n  vertical-align: middle;\n  position: relative;\n}\n.buttonxxxx1{\n  border-radius: 50%;\n  width: 15px;\n  height: 15px;\n  padding: 5px;\n  /*border: black .1px solid;*/\n}\n.button_submit{\n  width: 100%;\n}\n.el-date /deep/  .el-calendar-table .el-calendar-day{\n  width: 60px;\n  height: 40px;\n}\n.el_drawer_pics{\n}\n.div_weather{\n  margin-top: 15px;\n  background-color: #c3dad9;\n  padding: 10px 10px 10px 13px;\n  border-radius: 10px;\n}\n.button_weather{\n  border-radius: 50%;\n  width: 27px;\n  height: 27px;\n  padding: 5px;\n}\n.button_4{\n  margin-left: 10px;\n  width:50px;\n  height:50px\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}