{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport SideImage from '@/components/SideImage.vue';\nimport EditorDiary from '@/components/EditorDiary.vue';\nexport default {\n  name: \"DiaryView\",\n  components: {\n    EditorDiary,\n    SideImage\n  },\n  data() {\n    return {\n      input_search: '',\n      reverse: true,\n      directories: [],\n      score: null,\n      search_date: new Date(),\n      diaryForm: {\n        id: null,\n        info: '',\n        title: null,\n        article: '',\n        create_date: null,\n        update_date: null,\n        color: '',\n        weather: '',\n        score: null,\n        collection: null,\n        shit: null,\n        delete: null,\n        manager_id: null\n      },\n      count: 0,\n      icons: {\n        order: 'el-icon-bottom' // 向下箭头\n      },\n\n      show: {\n        show_color: false,\n        show_aside_title: true,\n        show_date: false,\n        fullscreen: false,\n        show_img: false,\n        show_editor: false\n      },\n      colors: {\n        face: ['#99A9BF', '#F7BA2A', '#FF9900']\n      },\n      max: 7,\n      value: null\n    };\n  },\n  mounted() {\n    this.getDirectory();\n  },\n  methods: {\n    click_delete_diary() {\n      if (this.diaryForm.id == 0) {\n        this.$message.error('错了哦，这是一条错误消息');\n      } else {\n        this.$confirm('此操作将删除该日记，是否继续？', '提示', {\n          confirmButtonText: '确定',\n          cancelButtonText: '取消',\n          type: 'error'\n        }).then(() => {\n          this.deleteRequest('/diary/delete/' + this.diaryForm.id).then(resp => {\n            if (resp.data.code == 200) {\n              this.$message({\n                type: 'success',\n                message: '删除成功!'\n              });\n              this.init_diary_form();\n              this.getDirectory();\n            } else {\n              this.$message.error('删除失败，可能您未选中文章，或已经提交过删除请求了！');\n            }\n          }).catch(() => {\n            this.$message({\n              type: 'info',\n              message: '已取消删除'\n            });\n          });\n        });\n      }\n    },\n    goNewDiary() {\n      this.$router.push('/newDiary');\n    },\n    clickTest() {\n      this.getRequest('/diary/getArticle/2').then(resp => {\n        if (resp) {\n          alert(JSON.stringify(resp));\n        }\n      });\n    },\n    getDirectory() {\n      this.getRequest('/diary/directory/all').then(resp => {\n        if (resp) {\n          this.makeDirectories(resp, this.max);\n        }\n      });\n    },\n    clickDiaryName(id) {\n      this.getRequest('/diary/article/' + id).then(resp => {\n        if (resp) {\n          this.diaryForm = resp.data.obj;\n          var md_content = \"Hello.\\n\\n* This is markdown.\\n* It is fun\\n* Love it or leave it.\";\n          document.getElementById('content').innerHTML = marked.parse('# Marked in the browser\\n\\nRendered by **marked**.');\n          console(this.diaryForm.article);\n        }\n      });\n    },\n    click_order() {\n      if (this.reverse == true) {\n        this.reverse = false;\n        this.icons.order = 'el-icon-top';\n      } else {\n        this.reverse = true;\n        this.icons.order = 'el-icon-bottom';\n      }\n    },\n    click_show_color() {\n      this.show.show_color = !this.show.show_color;\n    },\n    click_button_color(color) {\n      this.getRequest('/diary/directory/color/' + color).then(resp => {\n        if (resp) {\n          this.makeDirectories(resp, this.max);\n        }\n      });\n    },\n    click_button_editor() {\n      this.$router.push({\n        path: '/edit',\n        query: {\n          did: this.diaryForm.id\n        }\n      });\n    },\n    //   // alert(this.$store.state.editor_diary_page)\n    //   if(this.$store.state.editor_diary_page == true){\n    //     this.$store.commit('show_editor_page_false');\n    //   }else{\n    //     this.$store.commit('show_editor_page_true');\n    //   }\n    //   // alert(this.$store.state.editor_diary_page)\n    // },\n    click_show_img() {\n      this.show.show_img = !this.show.show_img;\n    },\n    makeDirectories(resp, max) {\n      for (var i = 0, len = resp.data.length; i < len; i++) {\n        if (resp.data[i].title.length >= max) {\n          resp.data[i].title = resp.data[i].title.substring(0, max) + ' ···';\n        }\n      }\n      this.directories = resp.data;\n    },\n    click_collection() {\n      this.getRequest('/diary/directory/collection').then(resp => {\n        if (resp) {\n          this.makeDirectories(resp, this.max);\n        }\n      });\n    },\n    click_rate() {\n      this.getRequest('/diary/directory/score/' + this.score).then(resp => {\n        if (resp) {\n          this.makeDirectories(resp, this.max);\n        }\n      });\n    },\n    clickENTER() {\n      this.getRequest('/diary/search/directory/' + this.input_search).then(resp => {\n        this.makeDirectories(resp, this.max);\n      });\n    },\n    click_show_date() {\n      this.show.show_date = !this.show.show_date;\n    },\n    init_diary_form() {\n      this.diaryForm.id = null;\n      this.diaryForm.info = '';\n      this.diaryForm.title = null;\n      this.diaryForm.article = '';\n      this.diaryForm.create_date = null;\n      this.diaryForm.update_date = null;\n      this.diaryForm.color = '';\n      this.diaryForm.weather = '';\n      this.diaryForm.score = null;\n      this.diaryForm.collection = null;\n      this.diaryForm.shit = null;\n      this.diaryForm.delete = null;\n      this.diaryForm.manager_id = null;\n    }\n  }\n};","map":{"version":3,"mappings":";AA+GA;AACA;AACA;EACAA;EACAC;IACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;MACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAV;QACAW;QACAC;QACAC;QACAC;MACA;MACAC;MACAC;QACAC;MACA;;MACAC;QACAC;QACAC;QACAC;QACAC;QACAC;QACAC;MAEA;MACAC;QACAC;MACA;MACAC;MACAC;IACA;EACA;EACAC;IACA;EACA;EACAC;IACAC;MACA;QACA;MACA;QACA;UACAC;UACAC;UACAC;QACA;UACA;YACA;cACA;gBACAA;gBACAC;cACA;cACA;cACA;YACA;cAC