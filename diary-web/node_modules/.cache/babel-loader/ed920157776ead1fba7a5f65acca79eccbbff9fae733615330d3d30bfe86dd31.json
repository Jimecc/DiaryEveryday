{"ast":null,"code":"export default {\n  name: \"LoginView\",\n  data() {\n    return {\n      loginForm: {\n        username: '',\n        password: '',\n        code: '1234'\n      },\n      rules: {\n        username: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }]\n        // code: [{required: true, message: '请输入验证码', trigger: 'blur'}]\n      }\n    };\n  },\n\n  methods: {\n    click_submit() {\n      // this.$refs.loginForm.validate((valid)=>{\n      //   if(valid){\n      this.loading = true;\n      this.postRequest('/register', this.loginForm).then(resp => {\n        if (resp) {\n          this.loading = false;\n          //清空菜单\n          // 存储用户 token\n          const tokenStr = resp.obj.tokenHead + resp.obj.token;\n          alert(JSON.stringify(resp));\n          window.sessionStorage.setItem('tokenStr', tokenStr);\n\n          // 跳转页面\n          let path = this.$route.query.redirect;\n          this.$router.replace(path == '/' || path == undefined ? '/home' : path);\n        }\n      });\n    },\n    click_img_1() {\n      window.location.href = 'https://github.com/Jimecc/';\n    },\n    click_img_2() {\n      window.location.href = 'https://github.com/Jimecc/';\n    }\n  }\n};","map":{"version":3,"mappings":"AAuCA;EACAA;EACAC;IACA;MACAC;QACAC;QACAC;QACAC;MACA;MACAC;QACAH;UAAAI;UAAAC;UAAAC;QAAA;QACAL;UAAAG;UAAAC;UAAAC;QAAA;QACA;MACA;IACA;EACA;;EACAC;IACAC;MACA;MACA;MACA;MACA;QACA;UACA;UACA;UACA;UACA;UACAC;UACAC;;UAEA;UACA;UACA;QACA;MAEA;IAEA;IACAC;MACAD;IACA;IACAE;MACAF;IACA;EACA;AACA","names":["name","data","loginForm","username","password","code","rules","required","message","trigger","methods","click_submit","alert","window","click_img_1","click_img_2"],"sourceRoot":"src/views","sources":["LoginView.vue"],"sourcesContent":["<template>\n<!--<div>-->\n\n<!--  <el-card class=\"box-card\">-->\n<!--    <div slot=\"header\" class=\"clearfix\">-->\n<!--      <h2>登录</h2>-->\n<!--&lt;!&ndash;      <el-button style=\"float: right; padding: 3px 0\" type=\"text\">操作按钮</el-button>&ndash;&gt;-->\n<!--    </div>-->\n<!--    <div v-for=\"o in 4\" :key=\"o\" class=\"text item\">-->\n<!--      {{'列表内容 ' + o }}-->\n<!--    </div>-->\n<!--  </el-card>-->\n<!--</div>-->\n<div class=\"div1\">\n  <div class=\"div2\" :rules=\"rules\" ref=\"loginForm\" :model=\"loginForm\">\n\n\n    <h1 style=\"margin-top: 40px\">登录</h1>\n    <el-divider>每日一记</el-divider>\n    <el-input prefix-icon=\"el-icon-s-custom\" v-model=\"loginForm.username\" class=\"input_1\" placeholder=\"请输入用户名\" ></el-input>\n    <el-input prefix-icon=\"el-icon-key\" v-model=\"loginForm.password\" class=\"input_1\" placeholder=\"请输入密码\" show-password></el-input>\n    <el-button type=\"primary\" @click=\"click_submit\" class=\"button-submit\">登&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;录</el-button>\n    <el-button type=\"text\" style=\"width: 20%;text-align: left;margin-top: 30px\">注册</el-button>\n    <el-button type=\"text\" style=\"margin-left: 30%\">忘记密码</el-button>\n    </div>\n\n    <div class=\"div-3\">\n      <el-tooltip content=\"Github\" placement=\"top\">\n          <img @click=\"click_img_1\" class=\"img-1\" src=\"../assets/github-black.png\">\n      </el-tooltip>\n      <el-tooltip content=\"Gitee\" placement=\"top\">\n        <img @click=\"click_img_2\" class=\"img-1\" src=\"../assets/gitee.png\">\n      </el-tooltip>\n    </div>\n\n</div>\n</template>\n\n<script>\nexport default {\n  name: \"LoginView\",\n  data() {\n    return {\n      loginForm:{\n        username: '',\n        password: '',\n        code: '1234'\n      },\n      rules: {\n        username: [{required: true, message: '请输入用户名', trigger: 'blur'}],\n        password: [{required: true, message: '请输入密码', trigger: 'blur'}],\n        // code: [{required: true, message: '请输入验证码', trigger: 'blur'}]\n      }\n    }\n  },\n  methods:{\n    click_submit(){\n      // this.$refs.loginForm.validate((valid)=>{\n      //   if(valid){\n          this.loading = true;\n          this.postRequest('/register',this.loginForm).then(resp=>{\n            if(resp){\n              this.loading=false;\n              //清空菜单\n              // 存储用户 token\n              const tokenStr = resp.obj.tokenHead+resp.obj.token;\n              alert(JSON.stringify(resp));\n              window.sessionStorage.setItem('tokenStr',tokenStr);\n\n              // 跳转页面\n              let path = this.$route.query.redirect;\n              this.$router.replace((path=='/' || path == undefined)?'/home':path);\n            }\n\n          })\n\n      },\n    click_img_1(){\n      window.location.href = 'https://github.com/Jimecc/';\n    },\n    click_img_2(){\n      window.location.href = 'https://github.com/Jimecc/';\n    }\n  }\n}\n</script>\n\n<style scoped>\n.box-card{\n  width: 350px;\n  height: 500px;\n  /*background-color: blanchedalmond;*/\n  margin: 10% 0 0 38%;\n  border-radius: 20px;\n  border: blanchedalmond 1px solid;\n  text-align: center;\n  background: rgba(255, 255, 255, .4);\n}\n.div1{\n  /*text-align: center;*/\n  width: 100%;\n  height: 100%;\n  position: absolute;\n  /*background-color: blanchedalmond;*/\n  background-image: url('../assets/bgimg.png');\n  background-size: cover;\n  /*background: linear-gradient(blue,green);*/\n  /* 背景渐变色 - 原理2 */\n  /*background: linear-gradient(-45bgimg1.pngdeg, #ee7752, #e73c7e, #23a6d5, #23d5ab);*/\n  /*!* 背景尺寸 - 原理3 *!*/\n  /*background-size: 600% 600%;*/\n  /*!* 循环动画 - 原理4 *!*/\n  /*animation: gradientBG 10s ease infinite;*/\n}\n@keyframes gradientBG {\n  0% {\n    background-position: 0% 50%;\n  }\n  50% {\n    background-position: 100% 50%;\n  }\n  100% {\n    background-position: 0% 50%;\n  }\n}\n.div2{\n  width: 350px;\n  height: 500px;\n  /*background-color: #fff;*/\n  margin: 10% 0 0 40%;\n  border-radius: 20px;\n  border: coral 1px solid;\n  text-align: center;\n  background: rgba(255, 255, 255, 1);\n}\n.color_1{\n\n}\n.input_1{\n  width: 80%;\n  border-radius: 100px;\n  margin-top: 40px;\n}\n.input_1>>>.el-input__inner { /*或者 .s2>>>.el-input__inner  */\n  border-radius: 20px; /*输入框圆角值*/\n  border: coral .1px solid;\n}\n.button-submit{\n  width: 80%;\n  margin-top: 40px;\n  border-radius: 80px;\n}\n.div-3{\n  height: 100%;\n  width: 100%;\n  position: absolute;\n  /*background-color: #42b983;*/\n  text-align: right;\n}\n.img-1{\n  width: 40px;\n  margin-top: 11%;\n  margin-right: 40px;\n  border-radius: 150px;\n  box-shadow: 0 0 7px black ;\n  border: #ffbc03 2px solid;\n}\n\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}