{"ast":null,"code":"import { marked } from 'marked';\nimport { debounce } from 'lodash-es';\nexport default {\n  data: () => ({\n    input: '# hello'\n  }),\n  computed: {\n    output() {\n      return marked(this.input);\n    }\n  },\n  methods: {\n    update: debounce(function (e) {\n      this.input = e.target.value;\n    }, 100)\n  }\n};","map":{"version":3,"mappings":"AACA;AACA;AAEA;EACAA;IACAC;EACA;EACAC;IACAC;MACA;IACA;EACA;EACAC;IACAC;MACA;IACA;EACA;AACA","names":["data","input","computed","output","methods","update"],"sourceRoot":"src/components","sources":["DiaryArticle.vue"],"sourcesContent":["<script>\nimport { marked } from 'marked'\nimport { debounce } from 'lodash-es'\n\nexport default {\n  data: () => ({\n    input: '# hello'\n  }),\n  computed: {\n    output() {\n      return marked(this.input)\n    }\n  },\n  methods: {\n    update: debounce(function (e) {\n      this.input = e.target.value\n    }, 100)\n  }\n}\n</script>\n\n<template>\n  <div class=\"editor\">\n    <textarea class=\"input\" :value=\"input\" @input=\"update\"></textarea>\n    <div class=\"output\" v-html=\"output\"></div>\n  </div>\n</template>\n\n<style>\nbody {\n  margin: 0;\n}\n\n.editor {\n  height: 100vh;\n  display: flex;\n}\n\n.input,\n.output {\n  overflow: auto;\n  width: 50%;\n  height: 100%;\n  box-sizing: border-box;\n  padding: 0 20px;\n}\n\n.input {\n  border: none;\n  border-right: 1px solid #ccc;\n  resize: none;\n  outline: none;\n  background-color: #f6f6f6;\n  font-size: 14px;\n  font-family: 'Monaco', courier, monospace;\n  padding: 20px;\n}\n\ncode {\n  color: #f66;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}